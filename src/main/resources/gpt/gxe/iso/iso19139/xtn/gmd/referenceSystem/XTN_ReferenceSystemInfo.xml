            <g:body>
              <g:element g:value="4326"/>
            </g:body>
          </g:element>
          
          <g:element g:targetName="gmd:codeSpace" g:minOccurs="0" g:maxOccurs="1"
            g:extends="$base/schema/gco/basicTypes/CharacterString_PropertyType.xml"/> 
                      
        </g:element>
